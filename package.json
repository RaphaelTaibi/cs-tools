{
  "name": "cs-tools-kit-workspace",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "packages/*",
    "apps/*"
  ],
  "scripts": {
    "dev": "pnpm -C apps/demo-react dev",
    "build": "pnpm -r build",
    "build:react": "pnpm -C packages/react build",
    "build:core": "pnpm -C packages/core build",
    "lint": "pnpm -r lint",
    "lint:fix": "pnpm -r lint:fix",
    "test": "node scripts/run-tests.mjs",
    "test:coverage": "node scripts/run-tests.mjs --coverage",
    "storybook": "pnpm -C packages/react storybook",
    "build-storybook": "pnpm -C packages/react build-storybook"
  }
}
