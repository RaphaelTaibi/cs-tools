{
  "name": "@cs-tools/core",
  "version": "0.1.0",
  "type": "module",
  "description": "Framework-agnostic utilities for cs-tools-kit.",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/RaphaelTaibi/cs-tools.git",
    "directory": "packages/core"
  },
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "build:types": "tsc -p tsconfig.json --emitDeclarationOnly",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage"
  },
  "keywords": [
    "typescript",
    "utilities",
    "toolkit",
    "validators",
    "formatters",
    "helpers"
  ],
  "author": "Taibi Raphael",
  "license": "MIT",
  "devDependencies": {
    "@eslint/js": "^9.37.0",
    "@vitest/coverage-v8": "3.2.4",
    "eslint": "^9.37.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.46.1",
    "vitest": "^3.2.4"
  }
}
